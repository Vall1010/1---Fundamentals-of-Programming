public class Bus {
    public static void main(String[] args) {
        
        int passengers=0;
        String text ="Number of Passengers: ";

        passengers +=9 ;
        System.out.println(text+passengers);

        passengers-=5;
        System.out.println(text+passengers);
   
        passengers-=4;
        System.out.println(text+passengers);
    }
}
