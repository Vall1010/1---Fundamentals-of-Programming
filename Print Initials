public class YourInitials {
    public static void main(String[] args) {
        //print initials here. 
    System.out.println("V           V   S S S");
    System.out.println(" V         V  S       S");
    System.out.println("  V       V   S");    
    System.out.println("   V     V      S SS S");
    System.out.println("    V   V              S");    
    System.out.println("     V V      S       S");
    System.out.println("      V         S S S");    
}
}
